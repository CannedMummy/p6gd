{
   "build": {
      "options": {
         "collapseInlineTagWhitespace": true,
         "collapseWhitespace": true,
         "minifyCSS": true,
         "minifyJS": true,
         "minifyURLs": true,
         "removeComments": true,
         "sortAttributes": true,
         "sortClassName": true
      },
      "cwd": "public/html",
      "dest": "public/html",
      "expand": true,
      "ext": ".min.html",
      "src": [ "*.html" ]
   }
}